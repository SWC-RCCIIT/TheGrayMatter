<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="600sp"
    android:layout_marginTop="50sp"
    android:background="@drawable/prescriptionformat"
    tools:context=".fourthactivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="200sp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="80sp"
        android:layout_marginTop="70sp"
        android:textAllCaps="true"
        android:textStyle="bold"

        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="121dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="50sp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="330sp"
        android:layout_marginTop="70sp"
        android:gravity="center"
        android:textAllCaps="true" />

    <TextView
        android:id="@+id/diag"
        android:layout_width="260sp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="150sp"
        android:layout_marginTop="130sp"
        android:textAllCaps="true"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/sym"
        android:layout_width="260sp"
        android:layout_height="80sp"
        android:layout_marginLeft="150sp"
        android:layout_marginTop="175sp"
        android:textAllCaps="true"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/drg"
        android:layout_width="200sp"
        android:layout_height="50sp"
        android:layout_marginLeft="200sp"
        android:layout_marginTop="280sp"
        android:textAllCaps="true"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/dos"
        android:layout_width="200sp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="160sp"
        android:layout_marginTop="350sp"
        android:textAllCaps="true" />

    <TextView
        android:id="@+id/per"
        android:layout_width="200sp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="160sp"
        android:layout_marginTop="400sp"
        android:textAllCaps="true"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/adv"
        android:layout_width="250sp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="140sp"
        android:layout_marginTop="450sp"
        android:textAllCaps="true"
        android:textStyle="bold" />
</RelativeLayout>